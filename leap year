#include<stdio.h>
int main()
{
	int year;
	printf("enter the year:");
	scanf("%d",&year);
	if(year %400==0)
	{
		printf(" is leap year:");
	}
	else if(year %100==0)
	{
		printf(" is not a leap year:");
	}
	else if(year %4==0)
	{
		printf(" is a leap year:");
	}
	else
	{
		printf(" is a leap year;");
	}
	return 0;
}
